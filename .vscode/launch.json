{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch sync.Demo.Catalog",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build sync.Demo.Catalog",
            "program": "${workspaceFolder}/sync_demo/Demo.Catalog/src/Demo.Catalog.Service/bin/Debug/net7.0/Demo.Catalog.Service.dll",
            "args": [],
            "cwd": "${workspaceFolder}/sync_demo/Demo.Catalog/src/Demo.Catalog.Service",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}